{
    "name": "atla",
    "displayName": "Atla",
    "description": "A VS Code Integration extension for Atla.",
    "version": "0.0.1",
    "private": true,
    "author": "Tatsuya Nakano",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/tatsuno/atla-vscode"
    },
    "bugs": {
        "url": "https://github.com/tatsuno/atla-vscode/issues"
    },
    "engines": {
        "vscode": "^1.67.0"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -b",
        "watch": "tsc -b -w",
        "lint": "eslint ./src --ext .ts,.tsx"
    },
    "devDependencies": {
        "@types/vscode": "1.67.0",
        "@types/node": "^16.11.7",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "eslint": "^8.13.0",
        "mocha": "^9.2.1",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "vscode-languageclient": "8.0.1"
    },
    "activationEvents": [
        "onLanguage:atla"
    ],
    "main": "out/main",
    "contributes": {
        "languages": [
            {
                "id": "atla",
                "extensions": [
                    ".atla"
                ],
                "aliases": [
                    "Atla"
                ]
            }
        ]
    }
}